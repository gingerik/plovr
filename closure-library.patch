From 5df5f3f1a7a9fdffa24d590a8f420a7f8ba960f1 Mon Sep 17 00:00:00 2001
From: Erik Timmers <e.timmers@gmail.com>
Date: Mon, 3 Mar 2014 23:25:36 +0100
Subject: [PATCH] Evade plovr warnings

---
 closure/goog/fs/error.js               | 1 -
 closure/goog/net/corsxmlhttpfactory.js | 2 +-
 closure/goog/net/xmlhttp.js            | 2 +-
 closure/goog/structs/structs.js        | 2 --
 4 files changed, 2 insertions(+), 5 deletions(-)

diff --git a/closure/goog/fs/error.js b/closure/goog/fs/error.js
index c38db6c..ea70222 100644
--- a/closure/goog/fs/error.js
+++ b/closure/goog/fs/error.js
@@ -43,7 +43,6 @@ goog.fs.Error = function(error, action) {
 
   /**
    * @type {goog.fs.Error.ErrorCode}
-   * @deprecated Use the 'name' or 'message' field instead.
    */
   this.code;
 
diff --git a/closure/goog/net/corsxmlhttpfactory.js b/closure/goog/net/corsxmlhttpfactory.js
index 4f8f145..185fbc6 100644
--- a/closure/goog/net/corsxmlhttpfactory.js
+++ b/closure/goog/net/corsxmlhttpfactory.js
@@ -51,7 +51,7 @@ goog.inherits(goog.net.CorsXmlHttpFactory, goog.net.XmlHttpFactory);
 goog.net.CorsXmlHttpFactory.prototype.createInstance = function() {
   var xhr = new XMLHttpRequest();
   if (('withCredentials' in xhr)) {
-    return xhr;
+    return /** @type {!goog.net.XhrLike} */ (xhr);
   } else if (typeof XDomainRequest != 'undefined') {
     return new goog.net.IeCorsXhrAdapter();
   } else {
diff --git a/closure/goog/net/xmlhttp.js b/closure/goog/net/xmlhttp.js
index de954d0..df6a0d4 100644
--- a/closure/goog/net/xmlhttp.js
+++ b/closure/goog/net/xmlhttp.js
@@ -173,7 +173,7 @@ goog.net.DefaultXmlHttpFactory.prototype.createInstance = function() {
   if (progId) {
     return new ActiveXObject(progId);
   } else {
-    return new XMLHttpRequest();
+    return /** @type {!goog.net.XhrLike} */ (new XMLHttpRequest());
   }
 };
 
diff --git a/closure/goog/structs/structs.js b/closure/goog/structs/structs.js
index 9b527b0..99611bf 100644
--- a/closure/goog/structs/structs.js
+++ b/closure/goog/structs/structs.js
@@ -169,8 +169,6 @@ goog.structs.clear = function(col) {
  * @param {T=} opt_obj The object to be used as the value of 'this'
  *     within {@code f}.
  * @template T,S
- * @deprecated Use a more specific method (e.g. goog.array.forEach,
- *     goog.object.forEach, etc.)
  */
 goog.structs.forEach = function(col, f, opt_obj) {
   if (typeof col.forEach == 'function') {
-- 
1.9.0

