From d26f5cfe02025a7e7016e459b7bc6d7edffe3a3a Mon Sep 17 00:00:00 2001
From: Erik Timmers <erik.timmers@easygis.eu>
Date: Thu, 10 Apr 2014 09:25:07 +0200
Subject: [PATCH] Avert plovr warnings

---
 closure/goog/debug/logger.js           |    1 -
 closure/goog/net/corsxmlhttpfactory.js |    2 +-
 closure/goog/net/xmlhttp.js            |    2 +-
 3 files changed, 2 insertions(+), 3 deletions(-)

diff --git a/closure/goog/debug/logger.js b/closure/goog/debug/logger.js
index 5e52443..98c9609 100644
--- a/closure/goog/debug/logger.js
+++ b/closure/goog/debug/logger.js
@@ -346,7 +346,6 @@ goog.debug.Logger.Level.getPredefinedLevelByValue = function(value) {
  * name and should normally be based on the package name or class name of the
  * subsystem, such as goog.net.BrowserChannel.
  * @return {!goog.debug.Logger} The named logger.
- * @deprecated use goog.log instead. http://go/goog-debug-logger-deprecated
  */
 goog.debug.Logger.getLogger = function(name) {
   return goog.debug.LogManager.getLogger(name);
diff --git a/closure/goog/net/corsxmlhttpfactory.js b/closure/goog/net/corsxmlhttpfactory.js
index 4f8f145..185fbc6 100644
--- a/closure/goog/net/corsxmlhttpfactory.js
+++ b/closure/goog/net/corsxmlhttpfactory.js
@@ -51,7 +51,7 @@ goog.inherits(goog.net.CorsXmlHttpFactory, goog.net.XmlHttpFactory);
 goog.net.CorsXmlHttpFactory.prototype.createInstance = function() {
   var xhr = new XMLHttpRequest();
   if (('withCredentials' in xhr)) {
-    return xhr;
+    return /** @type {!goog.net.XhrLike} */ (xhr);
   } else if (typeof XDomainRequest != 'undefined') {
     return new goog.net.IeCorsXhrAdapter();
   } else {
diff --git a/closure/goog/net/xmlhttp.js b/closure/goog/net/xmlhttp.js
index de954d0..df6a0d4 100644
--- a/closure/goog/net/xmlhttp.js
+++ b/closure/goog/net/xmlhttp.js
@@ -173,7 +173,7 @@ goog.net.DefaultXmlHttpFactory.prototype.createInstance = function() {
   if (progId) {
     return new ActiveXObject(progId);
   } else {
-    return new XMLHttpRequest();
+    return /** @type {!goog.net.XhrLike} */ (new XMLHttpRequest());
   }
 };
 
